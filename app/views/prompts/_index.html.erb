<div class="container mt-3">
  <h6 class="m-2 py-3">AI generated content</h6>

  <% @prompts.each_with_index do |prompt, index| %>
    <div class="p-2 m-2 w-100 <%= index.odd? ? '' : 'bg-light text-dark' %>">
      <div>User input: <%= prompt.input %></div>

      AI generated response <%= prompt.id %>:<br><%= JSON.pretty_generate(prompt.output) %><br>

      <div class="my-2">
        AI generated response message: <%= prompt.message_content %><br>
      </div>
    </div>
  <% end %>
</div>
